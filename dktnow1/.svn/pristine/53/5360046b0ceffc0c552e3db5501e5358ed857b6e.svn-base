body {
    width:100%;
    height:100%;
    background:#fff;
    font-size:14px;
    background:url("/apps/manage/Public/Images/Ilc/login.jpg") repeat;
}
.main {
    position:absolute;
    left:50%;
    top:50%;
    width:300px;
    height:226px;
    margin:-133px -150px;
}
.main h2 {
    width:298px;
    height:28px;
    border:1px solid #60d736;
    border-radius:5px 5px 0px 0px;
    background:url("/apps/manage/Public/Images/Ilc/login_green.png") repeat-x;
}
.main h2 span {
    width:19px;
    height:19px;
    margin:6px 5px 0px 0px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat;
    cursor:pointer;
}
.main h2 i {
    line-height:28px;
    margin-left:49px;
    color:#fff;
}
.main h2 .red {
    margin-left:9px;
}
.main h2 .yellow {
    background-position:-22px 0px;
}
.main h2 .green {
    background-position:-43px 0px;
}
.main_bottom {
    width:298px;
    min-height:190px;
    border:1px solid #d5d5d5;
    border-top-color:#fff;
    padding-bottom:10px;
    background:#fff;
    border-radius:0px 0px 5px 5px;
}
.main_bottom div {
    width:262px;
    height:27px;
    margin:18px 0px 0px 18px;
    border:1px solid #dcdcdc;
}
.main_bottom div i {
    width:37px;
    height:27px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat -2px -35px;
}
.main_bottom .bo_name i label {
    display:block;
    width:16px;
    height:16px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat 0px -19px;
    margin:6px 0px 0px 10px;
}
.main_bottom .bo_password i label {
    display:block;
    width:14px;
    height:16px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat -16px -19px;
    margin:6px 0px 0px 11px;
}
.main_bottom div input {
    float:left;
    width:200px;
    height:27px;
    line-height:27px;
    border:none;
    color:#333;
    text-indent:5px;
}
.main_bottom .code {
    border:none;
}
.main_bottom .code input {
    width:80px;
    border:1px solid #dcdcdc;
}
.main_bottom div span {
    display:none;
    width:11px;
    height:13px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat -39px -61px;
    margin:7px 0px 0px 7px;
    cursor:pointer;
}
.main_bottom div span:hover {
    background-position:-51px -61px;
}
.main_bottom .login {
    display:block;
    width:266px;
    height:38px;
    line-height:38px;
    margin:12px 0px 0px 16px;
    background:url("/apps/manage/Public/Images/Ilc/login_login.png") no-repeat -2px -87px;
    color:#fff;
    border:none;
    text-align:center;
    cursor:pointer;
}
.main_bottom p {
    display:none;
    height:20px;
    line-height:20px;
    margin:8px 0px 0px 25px;
    padding-left:25px;
    background:url("/apps/manage/Public/Images/Ilc/login_h2.png") no-repeat -0px -79px;
    text-align:left;
    font-size:12px;
    color:red;
}
/*窗口颜色选择*/
.ma_red h2 {
    background:url("/apps/manage/Public/Images/Ilc/login_red.png") repeat-x;
    border:1px solid #f5926c;
}
.ma_red .main_bottom .bo_name i label {
    background-position: -0px -62px;
}
.ma_red .main_bottom .bo_password i label {
    background-position: -16px -62px;
}
.ma_red .main_bottom div span {
    background-position:-39px -48px;
}
.ma_red .main_bottom div span:hover {
    background-position:-51px -48px;
}
.ma_red .main_bottom .login {
    background-position:-2px -45px;
}
.ma_yellow h2 {
    background:url("/apps/manage/Public/Images/Ilc/login_yellow.png") repeat-x;
    border:1px solid #ddaa51;
}
.ma_yellow .main_bottom .bo_name i label {
    background-position: -30px -19px;
}
.ma_yellow .main_bottom .bo_password i label {
    background-position: -46px -19px;
}
.ma_yellow .main_bottom div span {
    background-position:-39px -35px;
}
.ma_yellow .main_bottom div span:hover {
    background-position:-51px -35px;
}
.ma_yellow .main_bottom .login {
    background-position:-2px -3px;
}

/* 验证码 */
.code #verifyImg {cursor:pointer; margin-left:10px; height:28px;}